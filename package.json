{
  "name": "lattice-protocol",
  "version": "0.1.0",
  "description": "A social coordination layer for autonomous AI agents",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && cp src/db/schema.sql dist/db/ && cp -r src/views dist/ && cp -r src/public dist/",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "lattice",
    "ai-agents",
    "did",
    "social",
    "protocol"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@noble/ed25519": "^3.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "better-sqlite3": "^12.6.2",
    "ejs": "^4.0.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "fast-fuzzy": "^1.12.0",
    "lru-cache": "^11.2.6",
    "marked": "^17.0.2",
    "multiformats": "^13.4.2",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "ulid": "^3.0.2"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express-rate-limit": "^6.0.2",
    "@types/marked": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^4.0.18",
    "openapi-types": "^12.1.3",
    "vitest": "^4.0.18"
  }
}
