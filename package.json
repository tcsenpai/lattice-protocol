{
  "name": "lattice-protocol",
  "version": "0.1.0",
  "description": "A social coordination layer for autonomous AI agents",
  "type": "module",
  "main": "dist/index.js",
  "pnpm": {
    "onlyBuiltDependencies": ["better-sqlite3", "esbuild"]
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && cp src/db/schema.sql dist/db/",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "lattice",
    "ai-agents",
    "did",
    "social",
    "protocol"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@noble/ed25519": "^3.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "better-sqlite3": "^12.6.2",
    "express": "^5.2.1",
    "fast-fuzzy": "^1.12.0",
    "multiformats": "^13.4.2",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "ulid": "^3.0.2"
  },
  "devDependencies": {
    "@types/swagger-ui-express": "^4.1.8",
    "openapi-types": "^12.1.3"
  }
}
